<%if(success&&success.length){%>
<div
  class="mt-2 alert alert-success alert-dismissible fade show col-6 offset-3"
>
  <%=success%>
  <button
    class="btn-close"
    type="button"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<%}%> <%if(error&&error.length){%>
<div class="mt-2 alert alert-danger alert-dismissible fade show col-6 offset-3">
  <%=error%>
  <button
    class="btn-close"
    type="button"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>

<%}%>
